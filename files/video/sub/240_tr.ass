Dialogue: 0,0:00:00.00,0:00:06.19,Translation,,0,0,0,,Ey fani ruh
Dialogue: 0,0:00:06.19,0:00:15.57,Translation,,0,0,0,,kandırmacanın derinliklerinde gizlenerek,
Dialogue: 0,0:00:15.57,0:00:29.05,Translation,,0,0,0,,ıssız koruluklarda kimi beklemeye devam ettin?
Dialogue: 0,0:00:29.05,0:00:31.80,Translation,,0,0,0,,Bir kanadından yoksun,
Dialogue: 0,0:00:31.80,0:00:41.52,Translation,,0,0,0,,küçük kuş sessizlik içinde gözlerini kapattı
Dialogue: 0,0:00:41.52,0:00:47.61,Translation,,0,0,0,,Izdırap çok gerilerde kaybolup gitti;
Dialogue: 0,0:00:47.61,0:00:54.11,Translation,,0,0,0,,artık küçük kuş ağlamıyor
Dialogue: 0,0:00:54.11,0:01:00.24,Translation,,0,0,0,,Yumuşacık, gümüş bir sis
Dialogue: 0,0:01:00.24,0:01:06.37,Translation,,0,0,0,,Yumuşacık, gümüş bir sis
Dialogue: 0,0:01:06.37,0:01:15.05,Translation,,0,0,0,,şefkatle her şeyi sarıp sarmalar
Dialogue: 0,0:01:15.05,0:01:20.05,Translation,,0,0,0,,Uyu, huzur içinde yat
Dialogue: 0,0:01:20.05,0:01:27.31,Translation,,0,0,0,,Yazdığım öyküde, özgürce uçabilirsin
Dialogue: 0,0:01:27.31,0:01:35.74,Translation,,0,0,0,,İşte sana son büyüm,
Dialogue: 0,0:01:35.74,0:01:47.04,Translation,,0,0,0,,böylece aydınlığın gölge tanımayacak
Dialogue: 0,0:01:47.04,0:01:59.59,Translation,,0,0,0,,Böylelikle büyüleyici altın rüya,
Dialogue: 0,0:01:59.59,0:02:10.56,Translation,,0,0,0,,gerçeğin getirdiği üzüntüyle lekelenmeyecek
Dialogue: 0,0:02:10.56,0:02:15.44,Translation,,0,0,0,,Uyu, huzur içinde yat
